<%= form_tag students_path do %>
<label> Student Form </label> <br>
  <label> First Name: </label> <br>
    <%= text_field_tag :'first_name' %> <br>
  <label> Last Name: </label> <br>
    <%= text_field_tag :'last_name' %> <br>

<label> Content: </label>
    <%= text_field_tag :'student[content]' %> <br>

  <div> Submit Student <% link_to new_student_url, new_student_url(@student) %></div>
  <% end %>
